        <rdfs:comment></rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://manusquare.project.eu/core-manusquare"/>
        <rdfs:label>CapabilityType</rdfs:label>
    </owl:Class>
    


    <!-- http://manusquare.project.eu/core-manusquare#CapacityAvailability -->

    <owl:Class rdf:about="http://manusquare.project.eu/core-manusquare#CapacityAvailability">
        <rdfs:subClassOf rdf:resource="http://manusquare.project.eu/core-manusquare#CoreClass"/>
        <rdfs:comment>In order to allow a management of the processes, and therefore of the resources and of the items, this class has been modeled in order to allow having a general vision of the availability of a certain resource implicated to a certain process. </rdfs:comment>
        <rdfs:isDefinedBy rdf:resource="http://manusquare.project.eu/core-manusquare"/>
        <rdfs:label>CapacityAvailability</rdfs:label>
    </owl:Class>
    


    <!-- http://manusquare.project.eu/core-manusquare#Ceramic -->

    <owl:Class rdf:about="http://manusquare.project.eu/core-manusquare#Ceramic">
